{
  "name": "justapis-javascript-sdk",
  "description": "A lightweight JavaScript SDK to connect to a JustAPIs gateway in Node or the browser.",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnyPresence/justapis-javascript-sdk.git"
  },
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "grunt test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "browser": {
    "http": "./lib/shims/http/http.js",
    "url": "./lib/shims/url/url.js",
    "./lib/parsers/xml": "./lib/shims/xml/xml.js",
    "./lib/cache/persistence/memory-storage": "./lib/cache/persistence/browser-storage"
  },
  "engines": {
    "node": ">=0.12"
  },
  "author": "AnyPresence",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "commitizen": "^2.8.2",
    "cz-conventional-changelog": "^1.1.6",
    "ghooks": "^1.2.3",
    "grunt": "^0.4.5",
    "grunt-browserify": "^4.0.1",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.7.0",
    "grunt-contrib-jshint": "^0.11.3",
    "grunt-contrib-uglify": "^0.11.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-mocha-test": "^0.12.7",
    "grunt-mochify": "^0.3.0",
    "mocha": "^2.3.4",
    "semantic-release": "^4.3.5",
    "sinon": "^1.17.2",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "native-promise-only": "^0.8.1",
    "tiny-emitter": "^1.0.2",
    "xml2js": "^0.4.15",
    "xmlserializer": "^0.3.3"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
